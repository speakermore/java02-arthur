<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="arthur.dao.fabulous.AttendanceMapper">
	<select id="addFabulous" parameterType="Fabulous">
		insert into fabulous(fabulousName,fabulousTime,studentName) f values(#{fabulousName},#{fabulousTime},#{studentName}) where f.#{studentId}=student.id 
	</select>
</mapper>