<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="arthur.dao.user.UserMapper">
	<select id="findByStuName"  resultType="Student">
		select * from student where studentName=#{studentName}
	</select>
	<select id="findByTeacherName" resultType="Teacher">
		select * from teacher where teacherName=#{teacherName}
	</select>
	<insert id="addStudent" parameterType="Student">
		insert into student(studentName,studentPwd,studentNo,studentSex,stuClass) 
		values (#{studentName},#{studentPwd},#{studentNo},#{studentSex},#{stuClass})
	</insert>
</mapper>