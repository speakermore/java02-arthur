<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="arthur.dao.AttendanceMapper">
	<insert id="addAttendanceSign" parameterType="Attendance">
		insert into attendance (studentId,studentSign,date) values (#{studentId},#{studentSign},#{date})
	</insert>
	<!-- <update id="updateAttendanceOut" parameterType="Attendance">
		update attendance set studentOut=#{date} where studentId=#{studentId}
	</update> -->
</mapper>