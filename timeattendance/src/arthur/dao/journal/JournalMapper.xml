<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="arthur.dao.journal.JournalMapper" >
<insert id="addJournal">
    insert into journal (journalContent,journalTime,studentId,stuClass) 
    values 
    (#{journalContent},#{journalTime},#{studentId},#{stuClass})
</insert>
<select id="findAllJournal">
	select * from journal
</select>
</mapper>